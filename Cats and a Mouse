#include<stdio.h>
#include <stdlib.h>
int main()
{
    int q,x,y,z;
    scanf("%d",&q);
    for(int i=0;i<q;i++)
    {
         scanf("%d%d%d",&x,&y,&z);
         if(abs(z-x) > abs(z-y))
         {
         printf("Cat B \n");  
         }
         else  if(abs(z-x) < abs(z-y))
         {
        printf("Cat A \n");  
         }
         else
         {
         printf("Mouse C\n");
         }

    }
    return 0;
}
